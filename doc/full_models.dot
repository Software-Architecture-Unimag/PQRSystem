	"ApplicationRecord" -> "Status" [label="", arrowhead="none", arrowtail="onormal"]
	"ApplicationRecord" -> "Log" [label="", arrowhead="none", arrowtail="onormal"]
	"ApplicationRecord" -> "Answer" [label="", arrowhead="none", arrowtail="onormal"]
	"ApplicationRecord" -> "PqrFile" [label="", arrowhead="none", arrowtail="onormal"]
	"ApplicationRecord" -> "User" [label="", arrowhead="none", arrowtail="onormal"]
	"Rol" -> "User" [arrowtail=odot, arrowhead=crow, dir=both color="#1C8A05"]
	"ApplicationRecord" -> "Rol" [label="", arrowhead="none", arrowtail="onormal"]
	"ApplicationRecord" -> "Notification" [label="", arrowhead="none", arrowtail="onormal"]
	"Department" -> "User" [arrowtail=odot, arrowhead=crow, dir=both color="#92B7DD"]
	"ApplicationRecord" -> "Department" [label="", arrowhead="none", arrowtail="onormal"]
	"ApplicationRecord" -> "Pqr" [label="", arrowhead="none", arrowtail="onormal"]
