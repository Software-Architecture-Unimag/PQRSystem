	"PqrFile" [shape=Mrecord, label="{PqrFile|id :bigint(20)\lurl :varchar(255)\lcreated_at :datetime\lupdated_at :datetime\l}"]
	"User" [shape=Mrecord, label="{User|id :bigint(20)\lemail :varchar(255)\lencrypted_password :varchar(255)\lreset_password_token :varchar(255)\lreset_password_sent_at :datetime\lremember_created_at :datetime\lsign_in_count :int(11)\lcurrent_sign_in_at :datetime\llast_sign_in_at :datetime\lcurrent_sign_in_ip :varchar(255)\llast_sign_in_ip :varchar(255)\lfirst_name :varchar(255)\llast_name :varchar(255)\lphone_number :varchar(255)\lbirth_date :date\lcreated_at :datetime\lupdated_at :datetime\lrol_id :bigint(20)\ldepartment_id :bigint(20)\l}"]
