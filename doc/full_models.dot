	"Ability" [shape=record, label="{Ability|}"]
	"Notification" [shape=Mrecord, label="{Notification|id :bigint(20)\ldate :datetime\lmessage :varchar(255)\lpqr_id :bigint(20)\luser_id :bigint(20)\lcreated_at :datetime\lupdated_at :datetime\l}"]
	"Department" [shape=Mrecord, label="{Department|id :bigint(20)\lname :varchar(255)\lcreated_at :datetime\lupdated_at :datetime\l}"]
	"Pqr" [shape=Mrecord, label="{Pqr|id :bigint(20)\lfirst_name :varchar(255)\llast_name :varchar(255)\lemail :varchar(255)\ltitle :varchar(255)\lsubject :varchar(255)\lbody :text\lcreation_date :datetime\lexpiration_date :datetime\lstatus_id :bigint(20)\lcreated_at :datetime\lupdated_at :datetime\l}"]
